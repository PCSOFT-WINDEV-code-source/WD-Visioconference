     code : |1+
      // Syntaxe :
      //Fermeture ()
      //
      // Paramètres :
      //	Aucun
      
      // 	bDemandeArret : Booléen permettant de savoir qui a demandé l'arrêt de la communication
      //					Vrai : c'est l'utilisateur de l'application
      //					Faux : c'est le correspondant
      Procédure Fermeture(bDemandeArret=Faux)
      
      // On envoie un message à l'autre utilisateur lui indiquant la fin de la communication
      si bDemandeArret alors EcrireMessage(csFINCOM)
      
      // Arrêt du thread d'écoute
      // Arrêt des threads
      ThreadArrête(gsNomThread,1000)
      ThreadArrête(csThreadFichier,1000)
      
      si SocketExiste(gsNomSocket) ALORS
      	// Fermeture de la socket d'écoute	
      	SocketFerme(gsNomSocket)	
      FIN
      
      si SocketExiste(csNomSocketFichier) = vrai alors
      	// fermeture de la socket de fichier
      	socketferme(csNomSocketFichier)
      fin
      
      // Remise à zéro des variables
      gbAccepteFichier = Faux
      gbFichierEnvoyéEnCours = faux
      gbFichierRecevoirEnCours = faux
      // Si la fenêtre principale est active, on met a jour la variable
      FEN_Principale.gfbenDialogue = faux	
      FEN_Principale.BTN_DIALOGUE..etat = actif
