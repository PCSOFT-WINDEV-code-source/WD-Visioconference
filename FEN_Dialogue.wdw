     name : _TraiteMessageFichier
     internal_properties : CAAAAAgAAADUL9Cmuig/+Qq6Rw3qBnuyf3R8PHKTLf7IIkyFffSiA9Z70Vyx6iDFsIiacL5GlR6UbCSFHi4Nw3BM5Q78/HQFZxCKKnzS3+I2fuoElpeok7A9K8PfYA0q5ot26K1e4P1ttEuRlmv/KGptmI9+mtP+h91R+NJf+iEqb76wLxuR/Ej6pChBYG/Ktht9qw52t84QXvn0L6Q=
     procedure_id : 1730380020784938689
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //_TraiteMessageFichier (<sMessage>)
      //
      // Paramètres :
      // 	sMessage : <indiquez ici le rôle de sMessage>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      Procédure _TraiteMessageFichier(sMessage)
      
      gsNomFichier = sMessage[[5 A ]]
      
      // Envoi d'un message à la fenêtre pour afficher le message correspondant
      PostMessage(Handle(FEN_Dialogue),"ERREUR",3,3)
      
      
      // On attend la réponse de l'utilisateur
      SI SignalAttend("SigOuiNon",3000) = Vrai ALORS	
      	
      	SI gbAccepteFichier = Vrai ALORS
      		// L'utilisateur a accepté
      		// Mise à jour de la variable permettant de savoir si un fichier est en attente de réception
      		gbFichierRecevoirEnCours = Vrai
      		
      		// On crée une socket
      		SI SocketExiste(csNomSocketFichier)= Vrai ALORS
      			SocketFerme(csNomSocketFichier)
      		FIN
      		// On crée une socket spéciale pour le transfert de fichier
      		SI SocketCrée(csNomSocketFichier,cnPortSocketFichier)  = Faux ALORS
      			
      			// On envoie un message d'erreur
      			gbFichierRecevoirEnCours = Faux
      			EcrireMessage(csFICHIER_ERREUR)
      			
      		SINON
      			// Avant d'exécuter le thread de lecture des messages, on vérifie s'il n'est pas encore en cours
      			SI ThreadEtat(csThreadFichier)=threadInexistant ALORS									
      				ThreadExécute(csThreadFichier,threadNormal,AttenteConnexionFichier)
      				// Envoi de la réponse
      				EcrireMessage(csACKOK)
      				FEN_Dialogue.BTN_ENVOIFICH..Etat = Grisé
      			FIN
      			
      			
      		FIN
      		gbAccepteFichier = Faux
      	SINON
      		// L'utilisateur à refusé, envoi de la réponse
      		gbAccepteFichier = Faux
      		gbFichierRecevoirEnCours = Faux
      		EcrireMessage(csFichierRefusé)				
      	FIN
      	
      FIN
