     name : GestionErreur
     internal_properties : CAAAAAgAAAAW6jAPJUw9XjI19mTcKGubBdJ7WT/6K6BuB9dnA2/rYRj7Sp5+8xDdoLhCWzBRH/RX9wC2J1sU2qMrENxPid5p6xeLVBDmHxCqqj+Ja32C5avm5kEW8NXwrDO76hskcPnLAlECSyRwMMA1DkVGO7a7TphfItMYHmla8TlgkKcf1NJBjKczH48iEdyOb1tizaxHBhrr
     procedure_id : 1621516287879324523
     type_code : 14
     code : |1+
      // Syntaxe :
      //GestionErreur ()
      //
      // Paramètres :
      //	Aucun
      
      //	nParam : Valeur
      // 	nCodeErreur : Paramètres permettant de différencier les messages d'erreur
      Procédure GestionErreur(nParam <utile>,nCodeErreur)
      
      sNomCorrespondant est une chaine		// Nom du correspondant
      
      // Selon le code d'erreur on affiche le bon message d'erreur
      selon nCodeErreur
      	
      	cas 1 // Arrêt de la communication demandé
      		DélaiAvantFermeture(500)
      		Info("Le correspondant a demandé l'arrêt de la communication")
      		DélaiAvantFermeture(0)
      		
      	cas 2 // Code d'erreur Refus du correspondant pour la réception d'un fichier
      		DélaiAvantFermeture(1000)
      		info("Votre correspondant ne désire pas recevoir de fichier")
      		DélaiAvantFermeture(0)
      		
      	cas 3	// Demande d'envoi de fichier de la part du correspondant
      		DélaiAvantFermeture(3000)
      		
      		// Recherche de la fiche du correspondants
      		HLitRecherchePremier(correspondants,IDCorrespondants,gnIdCorrespondants)
      		si htrouve(correspondants)=vrai alors
      			// On a trouvé le correspondant, on indique son nom
      			// Note : le nom est indiqué à partir de la liste des correspondants
      			sNomCorrespondant = correspondants.Nom + " " + correspondants.Prénom
      		SINON
      			// Le correspondant est inconnu	
      			sNomCorrespondant = FEN_Principale.gfsNom
      		FIN
      		// Demande de confirmation pour la réception du fichier
      		si ouinon(non,chaineconstruit("%1 désire vous envoyer le fichier suivant : %2",sNomCorrespondant,gsNomFichier) + rc + "Voulez vous l'accepter ?") = Oui ALORS
      			// mise a jour de la variable
      			gbAccepteFichier = Vrai
      			DélaiAvantFermeture(0)
      		sinon
      			gbAccepteFichier = faux
      		FIN
      		DélaiAvantFermeture(0)
      		// On modifie le signal pour dire au thread que la réponse est prête
      		SignalModifie("SigOuiNon",signalOuvert)
      		SignalModifie("SigOuiNon",signalFermé)
      		
      	cas 4	// Erreur lors du transfert du fichier
      		DélaiAvantFermeture(1000)
      		erreur("Une erreur s'est produite lors du transfert du fichier " + gsCheminFichier)
      		DélaiAvantFermeture(0)
      		
      	cas 5	// Transfert de fichier annulé par le correspondant
      		DélaiAvantFermeture(1000)
      		erreur("Votre correspondant a annulé le transfert du fichier " + gsNomFichier)
      		DélaiAvantFermeture(0)
      	cas 6 // demande d'ouverture du fichier que l'on a reçu
      		DélaiAvantFermeture(1000)
      		si ouinon("Voulez-vous ouvrir le fichier " + gsNomFichier + " ?")= oui alors
      			LanceAppliAssociée(gsNomFichier)
      		FIN
      		DélaiAvantFermeture(0)
      			 
      FIN
      
     type : 458752
