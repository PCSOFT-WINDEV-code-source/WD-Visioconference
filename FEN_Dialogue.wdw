      // Traitement automatique des erreurs :   exécuter le bloc de code CAS ERREUR
      //
      
      PROCEDURE FEN_Dialogue(gnIdCorrespondants,LOCAL gsNomSocket)
      
      gsNomThread est une chaine = "Thread" + gsNomSocket		// Nom du thread
      gbFichierEnvoyéEnCours est un booléen = Faux			// Variable permettant de savoir si un fichier est en cours d'envoi
      gsCheminFichier est une chaine							// Chemin du fichier à sauvegarder
      gbFichierRecevoirEnCours est un booléen = faux			// Variable permettant de savoir si un fichier est en cours de réception
      gbAccepteFichier est un booleen = faux					// réponse de l'utilisateur
      
      gsNomFichier est une chaine								
      gbArrêtFichier est un booléen							// Booléen permettant d'indiquer au thread d'envoi de fichier que l'utilisateur distant a annulé
      gbArrêtFichierDemandé est un booléen = faux
      
      
      
      
      // Mise en place d'un évènement pour gérer la fermeture de la fenêtre de "chat"
      Evénement(FinCommunication,"*.*","FinComm") 
      Evénement(GestionErreur,"*.*","ERREUR")
      
      
      // Recherche de la fiche du correspondant
      HLitRecherchePremier(correspondants,IDCorrespondants,gnIdCorrespondants)
      si htrouve(correspondants) = vrai alors
      	// On a trouvé le correspondant, on indique son nom
      	// Note : le nom est indiqué à partir de la liste des correspondants
      	
      	Libellé_Connexion = chaineconstruit("Connexion établie avec : %1 %2 ",correspondants.Nom , correspondants.prenom)
      	FEN_Principale.gfsNom = correspondants.Nom +" "+  correspondants.prenom
      SINON
      	// Le correspondant est inconnu	
      	Libellé_Connexion = Chaineconstruit("Connexion établie avec : %1",FEN_Principale.gfsNom)
      FIN
      
      CAS ERREUR:
      	// Erreur lors de la mise en place de l'évènement de gestion de la fenêtre de discussion ou 
      	// Lors de la mise en place de la gestion des messages d'erreurs
      	Erreur("Impossible de mettre en place les événements de gestion d'erreur et de fermeture de la fenêtre","L'application va se terminer.")
      	// L'application ne peut pas fonctionner sans cet événement
      	Ferme()
