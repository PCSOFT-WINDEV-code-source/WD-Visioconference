           code : |1-
            sListePolice est une chaine = ""				// Liste des polices installées sur la machine
            sPolice		 est une chaine = ""
            nIndice		 est un entier = 1
            
            // Supprimer tous les éléments de la liste
            ListeSupprimeTout(COMBO_Police)
            
            // Récupérer la liste des polices installées sur la machine
            sListePolice = policeliste()
            
            // Extraire la première police
            sPolice = ExtraitChaîne(sListePolice, 1, RC)
            
            tantque sPolice <> eot
            	// Ajouter la police dans la table	
            	ListeAjoute(COMBO_Police, sPolice)
            	
            	// Incrémenter l'indice
            	nIndice++
            	
            	// Extraire la police suivante
            	sPolice = ExtraitChaîne(sListePolice, nIndice, RC)
            FIN
            
            // Se positionner sur la police de type "Arial"
            ListeSelectPlus(COMBO_Police, ListeCherche(COMBO_Police, "Arial"))
            
            // Appeler par timer la procédure de mise en forme du texte
            ModifierMiseEnForme()
