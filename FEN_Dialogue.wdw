     name : LectureMessage
     internal_properties : CAAAAAgAAABRg4xDQIagGVdOYzW84bd6nt1nyJylshWTOZ3BkTecQPjLYFUEsP6089eguNLEtNllFTQMgRACZI6kBU0ErTjabiFr5YSKXFK0sksnximAQgkVYEQvgwHaiX21rTBbt8YDM3HhglHiXGtpHIh55PWIyv0Z6faZoAE5C5utO0ALd8lu7OBAZEnaCiiHAoFXg0BdZIDF6VQXLIZfARUXm5kQZKA=
     procedure_id : 1621516287879390059
     type_code : 14
     code : |1+
      // Syntaxe :
      //LectureMessage (<sNomSocketLect>)
      //
      // Paramètres :
      //	sNomSocketLect : Le nom de la socket sur laquelle il faut lire le message
      // Valeur de retour :
      // 	Aucune
      
      Procédure LectureMessage(sNomSocketLect)
      
      sMessage est une chaine			// Message reçu
      
      
      // On lit tant que le thread est actif
      // Note : On utilise une fonction bloquante pendant une durée déterminée dans la boucle : on ne va pas monopoliser le processeur
      BOUCLE	
      	// Lecture d'un message (fonction bloquante pendant une durée déterminée)	
      	sMessage = SocketLit(sNomSocketLect,Vrai)
      	
      	// Vérification du message
      	si Sansespace(sMessage)<>"" alors	
      		
      		// Il existe un message demandant l'arrêt de la communication
      		si sMessage[[A 3]]~=csFINCOM alors
      			
      			_TermineCommunication()
      			SORTIR
      			
      		SINON
      			
      			// Un fichier est en attente d'envoi, le message est donc la réponse du correspondant
      			si gbFichierEnvoyéEnCours = Vrai ALORS
      				_TraiteRéponseDemandeFichier(sMessage)
      			FIN
      			
      			si sMessage ~= csFichier_ERREUR ALORS
      				// un message d'erreur Fichier a été reçu
      				// On remet toutes les variables à 0
      				gbAccepteFichier = Faux
      				gbFichierEnvoyéEnCours = Faux
      				gbFichierRecevoirEnCours = Faux
      				
      				// ferme la socket
      				SocketFerme(csNomSocketFichier)
      				
      				// Envoi un message à la fenêtre pour afficher le message d'erreur correspondant
      				PostMessage(handle(FEN_Dialogue),"ERREUR",4,4)
      				BTN_ENVOIFICH..etat = actif
      			FIN
      			
      			// si le message est une annulation de fichier, on met la variable bArrêtFichier à faux
      			si sMessage~=csFichierAnnulé ALORS
      				gbArrêtFichier = vrai
      				PostMessage(handle(FEN_Dialogue),"ERREUR",5,5)
      				EcrireMessage("Transfert de fichier annulé")
      				BTN_ENVOIFICH..etat = actif
      			FIN
      
      			// Si le message est une demande d'envoi de fichier, on demande l'accord de l'utilisateur
      			si sMessage[[a 3]] ~= csFICHIER ALORS
      				_TraiteMessageFichier(sMessage)
      			sinon
      				Si sMessage[[a 3]]<>"100" et sMessage[[a 3]]<>"300" et gauche(sMessage,1)<>"#" et sMessage <> csFichierAnnulé  alors
      					SectionCritiqueDébut("AA")
      					smsg est une chaine
      					smsg = decrypte(sMessage[[5 a]],"",crypteAucun,vrai)
      					
      					_AjouteMessage(smsg)
      					
      
      					SectionCritiqueFin("AA")
      				fin
      			fin
      		FIN
      	FIN
      FIN
      
      // Arrêt du thread d'écoute
      ThreadArrête(gsNomThread,500)
      Multitache(-30)
     type : 458752
   -
     name : LectureMsgFichier
     procedure_id : 1621516287879455595
     type_code : 14
     code : |1-
      // Syntaxe :
      //LectureMsgFichier (<sNomCanal>, <nHndFichier>, <sNomFichier>)
      //
      // Paramètres :
      //	sNomCanal : Nom de la socket concernée
      //	nHndFichier : Handle du fichier dans lequel on va enregistrer les paquets
      //	sNomFichier : Le nom du fichier à écrire
      
      Procédure LectureMsgFichier(sNomCanal,nHndFichier,sNomFichier)
      
      sMessage est une chaine				// Message reçu
      sNomFichierEnvoyé est une chaine 	// Nom du fichier envoyé
      nTailleAEcrire est un entier		// Nombre d'octets à écrire
      nOctetsTotal est un entier			// Nombre d'octet total à écrire
      nOctetsEcris est un entier			// Nombre d'octets déjà écris
      sOctetsLus est une chaine			// Contenu du message
      
      
      // Mise à jour des champs relatifs au transfert de fichiers
      FEN_Dialogue.BTN_Annuler..visible = Vrai
      FEN_Dialogue.Jauge_Fichier..visible = vrai
      FEN_Dialogue.LIB_FICHIER..visible = vrai
      // Boucle
      boucle
      	// Récupération du message
      	sMessage = SocketLit(sNomCanal,faux,60000)
      	// Si il n'y a pas d'annulation en cours
          si FEN_Dialogue.gbArrêtFichier = faux alors
          	// Si le message est un message de fin de fichier
          	si sMessage= csFinFichier alors
          	
      	    	// On sort de la boucle
      	    	sortir
      	  	sinon
      	  		// Si le message est une annulation
      	   		si sMessage ~=csFichierAnnulé ALORS
      				FEN_Dialogue.gbArrêtFichier = vrai
      	   			// on sort de la boucle
      	   			sortir   			
      	   		fin
      	   		// Si le message est une erreur
      	   		si sMessage~=csFICHIER_ERREUR ALORS
      	   			// On met a jour la variable d'annulation pour exécuter le code de suppression du fichier 
      				FEN_Dialogue.gbArrêtFichier = vrai
      				sortir	
      	   		sinon
      	   			// Le message est valide
      	   			// Extraction du nom du fichier
      				sNomFichierEnvoyé =ExtraitChaîne(sMessage,2,"#") 
      				// Extraction de la taille totale du fichier
      		    	nOctetsTotal = val(ExtraitChaîne(sMessage,3,"#"))
      		    	// Extraction de la taille à écrire
      				nTailleAEcrire = val(extraitchaine(sMessage,4,"#"))
      				// Lecture et décryptage du contenu
      				sOctetsLus = decrypte(extraitchaine(sMessage,5,"#"),"",crypteAucun)
      				// On écrit dans le fichier
      				fecrit(nHndFichier,sOctetsLus,nTailleAEcrire)
      				// Incrémentation du nombre d'octets écrits
      		    	nOctetsEcris += nTailleAEcrire
      		    	
      		    	// Mise à jour du libellé
      				FEN_Dialogue.LIB_FICHIER = "Fichier en réception " + partieentiere(nOctetsEcris*100/nOctetsTotal) +" %"
      				// Mise a jour de la jauge
      		   		FEN_Dialogue.Jauge_Fichier..BorneMax = nOctetsTotal
      				FEN_Dialogue.Jauge_Fichier..valeur = nOctetsEcris 
      				// On laisse le temps à la fenêtre de se mettre à jour
      				MultitacheRepeint(FEN_Dialogue)
      	
      		   	fin
      	    fin
      	sinon
      		// Annulation 
      		sortir
      	fin
      FIN
      
      // On ferme le fichier
      fferme(nHndFichier)
      
      // On remet invisible les champs relatifs au transfert de fichier
      FEN_Dialogue.Jauge_Fichier..visible = faux
      FEN_Dialogue.LIB_FICHIER..visible = faux
      FEN_Dialogue.BTN_Annuler..visible = faux
      FEN_Dialogue.BTN_ENVOIFICH..etat = actif
      // On remet les valeurs à 0
      FEN_Dialogue.Jauge_Fichier..valeur = 0
      FEN_Dialogue.LIB_FICHIER = ""
      // Mise a jour de la variable des fichiers en attente de réception
      FEN_Dialogue.gbFichierRecevoirEnCours = faux
      si FEN_Dialogue.gbArrêtFichier = faux alors
      	// On envoie un message comme quoi le fichier est bien reçu
      	FEN_Dialogue.EcrireMessage("Fichier Reçu : " + gsNomFichier)
      	// On envoie un message à la fenêtre pour qu'elle affiche le message d'ouverture de fichier
      	PostMessage(handle(FEN_Dialogue),"ERREUR",6,6)
      sinon
      	// Annulation 
      	// On remet la variable à faux
      	FEN_Dialogue.gbArrêtFichier = Faux
      	// On supprime le fichier
      	fSupprime(sNomFichier)
      	si FEN_Dialogue.gbArrêtFichierDemandé = Vrai ALORS
      		// Envoi du message à l'autre utilisateur
      		FEN_Dialogue.EcrireMessage(csFichierAnnulé)
      		FEN_Dialogue.gbArrêtFichierDemandé = faux
      	sinon
      		// On envoie un message à la fenêtre pour qu'elle affiche le message d'annulation
      		PostMessage(handle(FEN_Dialogue),"ERREUR",5,5)
      	FIN
      fin
      // On ferme la socket
      socketferme(sNomCanal)
      // On arrête l'écoute
      ThreadArrête(csThreadFichier,100)
      multitache(-100)
