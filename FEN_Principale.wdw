   -
     name : EcouteRequêteDébutMessage
     internal_properties : CAAAAAgAAADGGsA/IVyldgoFHhQ0UOtLPQr7oXdiezBuf3evM49TuWjrgr5OgxiV6Fg6G/hZDwSv3/Ce3yNE+uMz4EQv6cZhy/cTtIAGp+gSyoNV94l+gU8Kih1KlHHE0A8ftseAnKUf1rWun5js9NRBInGCN6KXUmzzfncM+k3One2MLANb8EaVkLMn41u29fgyW9fuubx3Nsq7
     procedure_id : 1621516330831695209
     type_code : 14
     code : |1+
      Procédure EcouteRequêteDébutMessage(sNomCanal)
      
      sClientInfo est une chaine
      sMessage est une chaine
      
      // On regarde si un utilisateur s'est connecté
      sClientInfo = SocketClientInfo(sNomCanal,SocketAdresse)
      si gfbenDialogue = Vrai ALORS
      	socketecrit(sNomCanal,csNONDISPO)
      	socketferme(sNomCanal)
      	retour
      FIN
      // On analyse le résultat
      si Sansespace(sClientInfo)<>"" alors
      	// Recherche du correspondant dans le carnet d'adresse
      	HLitRecherchePremier(correspondants,Adresse,sClientInfo)
      	si hTrouve(correspondants)=faux alors
      		// Ce correspondant n'est pas reconnu
      		Message("Requête d'un utilisateur non reconnu : "+ sClientInfo)
      	sinon
      		gfsNom = correspondants.Nom +" " +  correspondants.prenom
      		
      	FIN
      		
      	sMessage = socketlit(sansespace(sNomCanal),faux,30000)
      	si sMessage[[5 a]] <> "" alors
      		
      		SI ErreurDétectée alors
      			// Erreur lors de l'envoi de l'acquittement
      			Erreur("Impossible de répondre à une requête d'un correspondant",ErreurInfo())		
      			SocketFerme(sNomCanal)
      		SINON
      			si gfsNom = ""
      				gfsNom = sMessage[[5 a]]
      			fin
      			PostMessage(handle(FEN_Principale),"CONNEXION",1,1)
      				
      			si SignalAttend("SIGCONNEXION", 2000)=vrai ALORS
      				
      				// On attend 50 centièmes pour être sur que si le signal doit être fermé, il est fermé
      				SectionCritiqueDébut()
      					// Sauvegarde de l'adresse en cours
      					gfsAdresseIP = sClientInfo
      				SectionCritiqueFin()
      				
      				// Envoi de l'acquittement	
      				SocketEcrit(Sansespace(sNomCanal),csACKOK)
      				
      				// Demande d'ouverture de la fenêtre de "chat"	
      				ExécuteThreadPrincipal(OuvertureChat,sNomCanal,0)
      				
      				
      			sinon
      				// Time out : Connexion refusée
      				SocketEcrit(sNomCanal,csREFUSE)
      				SocketFerme(sNomCanal)
      			fin
      		FIN
      	sinon
      		// Le nom est vide donc on refuse
      		SocketEcrit(Sansespace(sNomCanal),csREFUSE)
      	fin
      SINOn
      	Erreur("Impossible de déterminer l'adresse IP du correspondant",ErreurInfo())	
      FIN
     type : 458752
   -
     name : EnvoieRequêteDébutMessage
     procedure_id : 1621516330831760745
     type_code : 14
     code : |1+
      Procédure EnvoieRequêteDébutMessage(sIP)
      
      // On crée un nom de socket pour cette connexion
      sNomSocket est une chaine = DateduJour()+Maintenant()
      
      // Tentative de connexion
      Message("Connexion à l'adresse IP : "+sIP)
      si SocketConnecte(sNomSocket,cnPortSocketRequete,sIP) = faux
      
      	// Erreur de connexion	
      	Erreur("Impossible de joindre le correspondant",ErreurInfo());Message()
      	gfbenDialogue = Faux
      	BTN_DIALOGUE..etat = actif
      	RENVOYER Faux
      SINON
      	socketEcrit(sNomSocket,"100 " + NetNomMachine())
      	// Connexion établie, attente ACOK
      	Message("Connexion effectuée, Attente de la réponse du correspondant")
      	Selon SocketLit(sNomSocket,Faux,30000)
      		
      		CAs csACKOK :
      			// Connexion acceptée 
      			Message("Recherche du correspondants")
      			gfsnom=sIP
      			SignalDétruit("SIG_DISCUSSION")
      			signalcree("SIG_DISCUSSION",signalAutomatique,signalFermé)
      			
      			// Recherche du correspondant dans le carnet d'adresses
      			HLitRecherchePremier(correspondants,Adresse,sIP) 
      			si htrouve(Correspondants)=Vrai ALORS
      				// Demande d'ouverture de la fenêtre de "chat" avec le correspondant spécifié			
      				Message()
      				ExécuteThreadPrincipal(OuvertureChat,sNomSocket,correspondants.IDCorrespondants)	
      				
      			sinon
      				Message()
      				// Demande d'ouverture de la fenêtre de "chat" avec le correspondant spécifié			
      				ExécuteThreadPrincipal(OuvertureChat,sNomSocket,0)	
      				
      			FIN		
      			
      			gfbenDialogue = vrai					
      			RENVOYER Vrai
      		CAS csNonDispo :
      			// correspondant non disponible
      			Info("Le correspondant n'est pas disponible actuellement")
      			Message()
      			gfbenDialogue = Faux
      			BTN_DIALOGUE..etat = actif
      			RENVOYER Faux
      			
      		cas csrefuse
      			// correspondant a refusé		
      			Info("Le correspondant a refusé de vous prendre en discussion instantanée")
      			Message()
      			gfbenDialogue = Faux
      			BTN_DIALOGUE..etat = actif
      			RENVOYER Faux
      		AUTRES CAS :
      			// Pas de réponse de l'application du correspondant
      			Info("Le correspondant n'a pas répondu dans le délai imparti, abandon de la connexion à : "+sIP,ErreurInfo())	
      			Message()
      			gfbenDialogue = Faux
      			BTN_DIALOGUE..etat = actif
      			RENVOYER Faux		
      	FIN	
      FIN
     type : 458752
   -
     name : OuvertureChat
     procedure_id : 1621516330831826281
     type_code : 14
     code : |1-
      Procédure OuvertureChat(LOCAL sNomSocket,nIDCorrespondant)
      
      // recherche de l'adresse IP du correspondant
      sIP est une chaine
      HLitRecherchePremier(correspondants,IDCorrespondants,nIDCorrespondant)
      si htrouve(Correspondants)=vrai ALORS
      	sIP= Correspondants.Nom + " " + Correspondants.Prénom
      sinon
      	sIP = ""
      FIN
      // Ouverture de la fenêtre de communication avec le correspondant
      OuvreSoeur(FEN_Dialogue,nIDCorrespondant,sNomSocket)
