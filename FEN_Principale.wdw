          		si FluxAccepte(nIdFlux,ConfReception) = vrai ALORS
          			gfbEnComm =Vrai
          			BTN_APPEL..libelle = <§$0003§>
          			GPE_OPTIONS..etat = actif
          		FIN
          	sinon
          		FluxRefuse(nIdFlux)
          	FIN
          sinon
          	FluxRefuse(nIdFlux)	
          fin
