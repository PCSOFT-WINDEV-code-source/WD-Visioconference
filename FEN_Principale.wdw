   -
     name : DialogueConnexion
     internal_properties : CAAAAAgAAABPK5hMiC6aeq9vUU65Il12eAZyw98xzDw92dhr7ETtTbgmu1O1G5uUDiUoJAIVxThebNhw4zhq/JDr7jLTOZ95VhcGT4xyLuV4XdXekhCVzXRFcS8EXNEert/v6f4HE5lFTKDSbwxFnhWMOeMhGrHTVjAcJgG2gZljLKxkwSjL5QCwWkISEjXTeWxXMnjFhpMzzvSTxc9+sxv6/su1H2Q0FtVpfb/yFoK8+g08BKCFu7dqdCx5mkXG
     procedure_id : 1621516330831564137
     type_code : 14
     code : |1+
      // Syntaxe :
      //DialogueConnexion (<sParam>, <wparam>, <lparam>)
      //
      // Paramètres :
      //	sParam : Paramètre Windows pour l'appel d'une callback
      //	wparam : Paramètre Windows pour l'appel d'une callback
      //	lparam : Paramètre Windows pour l'appel d'une callback
      // Valeur de retour :
      // 	Aucune
      
      Procédure DialogueConnexion(sParam <utile>,wparam <utile>,lparam <utile>)
      selon lparam
      	cas 1 	// TimeOut de connexion, le correspondant n'a pas répondu dans les temps
      		// Affichage de la boîte de dialogue
      		DélaiAvantFermeture(2000)
      		// Demande de confirmation pour le dialogue en privé
      		si ouinon(non,CHaineconstruit("%1 voudrait avoir une discussion instantanée avec vous",gfsNom), "Acceptez vous ?")=Oui ALORS
      			DélaiAvantFermeture(0)
      			SignalModifie("SIGCONNEXION",signalOuvert)
      		sinon
      			DélaiAvantFermeture(0)
      			
      		FIN
      	cas 2 // Le correspondant a refusé
      		DélaiAvantFermeture(1000)
      		info("Le correspondant a refusé la discussion instantanée")
      		DélaiAvantFermeture(0)
      fin
      
