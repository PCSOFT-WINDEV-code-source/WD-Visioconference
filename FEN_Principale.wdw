     name : ModifieFlux
     procedure_id : 1621516330831891817
     type_code : 14
     code : |1+
      // Syntaxe :
      //ModifieFlux ()
      //
      
      Procédure ModifieFlux()
      nMode est un entier
      
      // Mise a jour de la variable nMode
      // Mode de transmission du flux
      si INT_ENVOI[1]= Vrai ALORS
      	nMode = FluxVidéoEmission
      FIN
      si INT_ENVOI[2]=vrai ALORS
      	nMode += FluxSonEmission
      FIN
      si INT_RECEPTION[1]=vrai ALORS
      	nMode += FluxVidéoRéception
      FIN
      si INT_RECEPTION[2]=vrai ALORS
      	nMode +=FluxSonRéception
      FIN
      // Stoppe le flux
      fluxstop(FluxRéception)
      // On le rejoue avec les nouveaux paramètres
      fluxjoue(FluxRéception,nMode)
